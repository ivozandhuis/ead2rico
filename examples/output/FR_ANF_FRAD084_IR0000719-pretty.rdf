      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Transaction entre Giraud Amic, seigneur de Châteauneuf et l'abbé de Saint-André de Villeneuve-lès-Avignon, portant sur la juridiction du lieu de Thouzon et la ville de Jonquerettes, haute justice, droits seigneuriaux, bans, cavalcades, corvées et autres obligations</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1695, t. 12, fol. 651-654 "Touzon, 15e tiroir, 3e liasse, n° 2 ".</html:p>
         </html:div>
      </rico:history>
      <rico:descriptiveNote rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Commentaire</html:head>
            <html:p>Manque n° 1 : donation par Raymond, comte de Toulouse à l'abbaye et confirmation par Alphonse de Toulouse (1088 et 1140).</html:p>
         </html:div>
      </rico:descriptiveNote>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>Transaction (1251, vidimus 1450) ; notice sur Touzon citant des actes relatifs aux droits des seigneurs du Thor sur Thouzon des années 1278 à 1468 (s. d., XVIIe siècle).</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
   <rico:RecordSet rdf:about="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#5-6-4-4-2">
