      <rico:hasRecordSetType rdf:resource="https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#File"/>
      <rico:hasOrHadIdentifier>
         <rico:Identifier>
            <rico:textualValue>2 E 9/1039</rico:textualValue>
            <rico:hasIdentifierType rdf:resource="https://example.com/standards/RiC/vocabularies/identifierTypes#05"/>
         </rico:Identifier>
      </rico:hasOrHadIdentifier>
      <rico:isAssociatedWithDate>
         <rico:Date>
            <rico:expressedDate>1343-1517</rico:expressedDate>
            <rico:normalizedValue>1343/1517</rico:normalizedValue>
         </rico:Date>
      </rico:isAssociatedWithDate>
      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Enclos des anciennes murailles, hors la porte des Tours, rue du Pertuis, rue de la Saunerie</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1525, t. 3, "aristote" et Inventaire de 1695, t. 14, fol. 234-236 "Orange 10e tiroir 17e-19e liasses".</html:p>
            <html:p>Vente de 1343 : Inventaire de 1525, t. 3, fol. 136 "aristote" "l".</html:p>
         </html:div>
      </rico:history>
      <rico:descriptiveNote rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Commentaire</html:head>
            <html:p>Vente du 13 décembre 1343 : bulle sur repli et lacs de soie rouge du prince, Raymond des Baux, diamètre 42 mm. Voir 11 NUM 374. Cat. notice 283.</html:p>
            <html:p>Achat au "portale de Torres" secus carreriam Domus rotunde (1517).</html:p>
            <html:p>rue du Pertuis : Pertusi (1343).</html:p>
            <html:p>19e liasse : acte de 1379 en mauvais état.</html:p>
         </html:div>
      </rico:descriptiveNote>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>Achat par Guillaume d'Ancézune d'une "grotte" ou "balma" au "portale de Torres" (1517) ; vente par Raymond de Mérindol à maître Bertrand Cordoani, notaire et à son fils Raymond d'une maison près de la rue du Pertuis (1343, bulle).</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
   <rico:RecordSet rdf:about="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#5-8-4-2-2">
