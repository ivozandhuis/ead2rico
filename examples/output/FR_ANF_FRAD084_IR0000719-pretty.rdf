      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Faubourg Meilleurat, faubourg des Tours, quartier de Guerinquières, Merdaric</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
