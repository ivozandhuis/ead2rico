            <rico:hasIdentifierType rdf:resource="https://example.com/standards/RiC/vocabularies/identifierTypes#05"/>
         </rico:Identifier>
      </rico:hasOrHadIdentifier>
      <rico:recordResourceExtent>registre papier de 280 fol., couvert parchemin, fin non foliotée.Oui</rico:recordResourceExtent>
      <rico:isAssociatedWithDate>
         <rico:Date>
            <rico:expressedDate>1408-1415</rico:expressedDate>
            <rico:normalizedValue>1408/1415</rico:normalizedValue>
         </rico:Date>
      </rico:isAssociatedWithDate>
      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Procédure devant la cour du Petit Scel de Montpellier par Peyronnette de Tremblay contre Etienne Albert, seigneur de Montel-de-Gelat et Roche-d'Agoux, héritier d'Auzias Alberti, seigneur du Thor, en récupération de clameurs sur sa dot, avec copie de pièces justificatives</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1695, t. 12, "Le Thor, 3e tiroir".</html:p>
         </html:div>
      </rico:history>
      <rico:descriptiveNote rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Commentaire</html:head>
            <html:p>Du 8 juin 1408 au 22 mai 1415.</html:p>
            <html:p>Montilio de Gelati et Ruppisdagulphi, Clarimontis diocesis. Copie d'actes, notamment : mariage d'Auzias Albert et Peyronette de Tremblay fol. 20 et en fin non foliotée.</html:p>
            <html:p>Testament d'Auzias Albert fol. 45 v° et en fin du registre</html:p>
            <html:p>Inventaire des biens de Guy et Auzias Aubert fol. 225 v°</html:p>
            <html:p>Testament de Raymond Albert en date du 16 mai 1390 fol. 255</html:p>
            <html:p>Testament de Guy Albert en date du 8 septembre 1370 fol. 263 v°</html:p>
            <html:p>Donation par Etienne Albert à son fils aîné Girbert, qu'il émancipe le 21 août 1403 (acte en français), des "châteaux et châtellenie del Tor" fol. 279.</html:p>
            <html:p>Hommage au pape par Raymond de Villeneuve au nom de son épouse Tiburge de Sabran (13 juillet 1364) en fin du registre.</html:p>
            <html:p>Historique du procès, voir Inventaire de 1695, t. 12, 3e tiroir, fol. 101-103.</html:p>
         </html:div>
      </rico:descriptiveNote>
   </rico:RecordSet>
