      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Titres spécifiques dans l'île d'Arnoux, provenant des seigneurs de Saint-Just</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Brouillard de l'inventaire, cahier 4.</html:p>
         </html:div>
      </rico:history>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
