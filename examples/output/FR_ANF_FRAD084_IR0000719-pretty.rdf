         <rico:Title>
            <rico:textualValue>Suite des procédures entre l'abbaye Saint-André de Villeneuve-lès-Avignon et le seigneur du Thor</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
