         <rico:Title>
            <rico:textualValue>Poursuites de Peyronnette de Tremblay contre Alix des Baux, comtesse d'Avelin</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1695, t. 12, "Le Thor, 3e tiroir".</html:p>
