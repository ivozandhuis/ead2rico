            <rico:hasIdentifierType rdf:resource="https://example.com/standards/RiC/vocabularies/identifierTypes#05"/>
         </rico:Identifier>
      </rico:hasOrHadIdentifier>
      <rico:recordResourceExtent>croquis : papier, 35 x 48 cm, sans orientation ni échelle ; "plan" du Thor : papier, 28,5 x 42,5 cm, échelle : 50 toises = 8,7 cm ; croquis de la Caudelière : papier, 34,5 x 46 cm.</rico:recordResourceExtent>
      <rico:isAssociatedWithDate>
         <rico:Date>
            <rico:expressedDate>1770-1773</rico:expressedDate>
            <rico:normalizedValue>1770/1773</rico:normalizedValue>
         </rico:Date>
      </rico:isAssociatedWithDate>
      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Plans et croquis du "canal de la Sorgue", du Thor, des terres de la Candelière (fin XVIIIe)</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:descriptiveNote rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Commentaire</html:head>
            <html:p>"Plan" du Thor, voir 12 NUM 164.</html:p>
         </html:div>
      </rico:descriptiveNote>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>Croquis du "canal de la Sorgue qui vient du Thor et dont partie coule vers Les Taillades, terroir du Thor et partie passe par dessus la prise noyée et découle vers Saint-Saturnin dans le terroir du Thor", confrontant les terres de Monsieur de Longchamp, celles de M. de Guast, celles de divers particuliers du Thor. "Plan du Thor" indiquant l'entrée de la ville, le grand canal de la réserve, le canal du moulin à blé, le canal du foulon, le chemin du moulin, celui de Saint-Michel ; croquis situant les terres de la Candelière, de l'hôpital, le pâti de la communauté, les mayres voisines et autres terres de divers particuliers.</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
