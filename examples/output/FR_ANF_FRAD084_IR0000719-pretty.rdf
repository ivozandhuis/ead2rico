      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Propriétés, provenant de Raymond Bernard Du Flamen, sises hors le portail Matheron, rue Carreterie, bourguet "des héritiers de Jean Cabassole ou bourguet des Ortolans"</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
