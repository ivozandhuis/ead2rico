      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Arpentements dans les îles et tableaux de défrichements</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>"Coté et paraphé n° 13 le 22 novembre 1865".</html:p>
         </html:div>
      </rico:history>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>"Cahier des arpentages dans les îles appartenant à la duchesse de Caderousse-Gramont, par Bernard, à partir du 22 mars 1816" ; arpentage donnant les noms des cantons, des fermiers, les contenances ("quantité de salmées") ; arpentage en lots de l'île de la Piboulette (1812), par nature de cultures (1819), arpentage de Cadenet (1817), croquis, arpentage des îles "pour le cadastre" (1824) ; arpentage des bois, par lots (1817) ; tableau des défrichements (1819) ; "livre d'arpentage" relatif aux îles et îlons (1er septembre 1837).</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
   <rico:RecordSet rdf:about="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#4-2-2-3-1-3">
      <rico:isOrWasIncludedIn rdf:resource="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#4-2-2-3-1"/>
      <rico:hasRecordSetType rdf:resource="https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#File"/>
      <rico:hasOrHadIdentifier>
