         <rico:Title>
            <rico:textualValue>Procédures et ajournement obtenu du parlement de Languedoc</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
