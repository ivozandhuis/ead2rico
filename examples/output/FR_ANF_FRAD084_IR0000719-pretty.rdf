         <rico:Title>
            <rico:textualValue>Lièves éparses et partielles des censes d'Orange</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
