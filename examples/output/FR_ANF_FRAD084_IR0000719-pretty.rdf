         <rico:Date>
            <rico:expressedDate>an XI-1825</rico:expressedDate>
            <rico:normalizedValue>1802/1825</rico:normalizedValue>
         </rico:Date>
      </rico:isAssociatedWithDate>
      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>"Commission spéciale des travaux de défense contre le Rhône" organisée par décret de 1810 pour la commune de Caderousse</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:descriptiveNote rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Commentaire</html:head>
            <html:p>Création par décret du 8 avril 1810, autorisation de sept membres par décret du 8 novembre 1810 ; président de la commission : Guérin, décédé en juin 1813, remplacé par Germanes.</html:p>
         </html:div>
      </rico:descriptiveNote>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>Arrêtés préfectoraux préparatoires, correspondance du maire de Caderousse, correspondance de la commission, extraits de délibération, questions juridiques, "mémoires de paiements", inventaires de 1813 et de 1823.</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
   <rico:RecordSet rdf:about="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#6-4">
      <rico:isOrWasIncludedIn rdf:resource="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#6"/>
      <rico:hasRecordSetType rdf:resource="https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#File"/>
      <rico:hasOrHadIdentifier>
