         <rico:Title>
            <rico:textualValue>Biens sis dans la ville de Caderousse</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1695 : 9e tiroir, liasses 1-2 et 10e tiroir, liasses 1-15.</html:p>
         </html:div>
      </rico:history>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>"Titres particuliers des directes et censes assises sur des propriétés, maison, cour, jardin, creux dans la ville" : achat, cession, recision de donation, transport de cense, nouveau bail, bail en paiement, lauzime, investiture, échange.</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
