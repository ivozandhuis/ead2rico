      <rico:hasRecordSetType rdf:resource="https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#File"/>
      <rico:hasOrHadIdentifier>
         <rico:Identifier>
            <rico:textualValue>2 E 9/478</rico:textualValue>
            <rico:hasIdentifierType rdf:resource="https://example.com/standards/RiC/vocabularies/identifierTypes#05"/>
         </rico:Identifier>
      </rico:hasOrHadIdentifier>
      <rico:isAssociatedWithDate>
         <rico:Date>
            <rico:expressedDate>1476-1577</rico:expressedDate>
            <rico:normalizedValue>1476/1577</rico:normalizedValue>
         </rico:Date>
      </rico:isAssociatedWithDate>
      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Au Chemin de Saint-Martin, aux Mattes de Saint-Martin, au Maton, au Maubastit ou Malbâti</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1695, t. 7, 2e vol., fol. (nv) 112-116 : "Caderousse, 14e tiroir, 30e-33e liasses".</html:p>
         </html:div>
      </rico:history>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>Achat par Marie de Crussol aux Mattes de Saint-Martin (1532) ; achat au Maton, de la directe de la sacristie de l'église Saint-Michel (1564).</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
   <rico:RecordSet rdf:about="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#5-1-2-2-3-12">
      <rico:isOrWasIncludedIn rdf:resource="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#5-1-2-2-3"/>
      <rico:hasRecordSetType rdf:resource="https://www.ica.org/standards/RiC/vocabularies/recordSetTypes#File"/>
      <rico:hasOrHadIdentifier>
         <rico:Identifier>
            <rico:textualValue>2 E 9/479</rico:textualValue>
            <rico:hasIdentifierType rdf:resource="https://example.com/standards/RiC/vocabularies/identifierTypes#05"/>
         </rico:Identifier>
      </rico:hasOrHadIdentifier>
      <rico:isAssociatedWithDate>
         <rico:Date>
            <rico:expressedDate>1270-1694</rico:expressedDate>
            <rico:normalizedValue>1270/1694</rico:normalizedValue>
         </rico:Date>
      </rico:isAssociatedWithDate>
      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>À la Maure vers Campredon, aux Mians</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1525, t. 2, "laomedon", "ysaac" Inventaire de 1695, t. 7, 2e vol., fol. (nv) 116-123 : "Caderousse, 14e tiroir, 34e-35e liasses".</html:p>
            <html:p>35e liasse :</html:p>
            <html:p>achat 1270 : Inventaire de 1525, t. 2, fol. 11 "laomedon" "a a" / Inventaire de 1695, t. 7, 2e vol., fol. (nv) 119 n° 1</html:p>
            <html:p>achat 1281 : Inventaire de 1525, t. 2, fol. 21 v° "ysaac" "b"</html:p>
            <html:p>achat 1288 : Inventaire de 1525, t. 2, fol. 3, "laomedon" "g". Inventaire de 695, t. 7, 2e vol., n° 3</html:p>
         </html:div>
      </rico:history>
      <rico:scopeAndContent rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Présentation du contenu</html:head>
            <html:p>Notamment : achat par Antoine de Lusignan de Claude Tacussel (1533) ; achat aux Mians par Raymond d'Ancézune de Pierre de Caderousse (1270) ; achat par Raimbaud d'Ancézune d'un bois situé "au-dessus du moulin des Bessons" (1288) ; échange entre Raimbaud d'Ancézune et Guillaume et Isnard Rebolli (1300) ; échange avec les héritiers de Pierre Canis (1304) ; bail à cultiver pour six ans par Raimbaud de Mornas, coseigneur en faveur de P. Querien de deux terres, aux Mians et à Fausse Loubes (1375).</html:p>
         </html:div>
      </rico:scopeAndContent>
   </rico:RecordSet>
   <rico:RecordSet rdf:about="http://data.archives-nationales.culture.gouv.fr/FRAD084_IR0000719#5-1-2-2-3-13">
