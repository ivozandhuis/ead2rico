      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Titres de censes et directes sur des propriétés, provenant de Raymond Bernard du Flamen, sises rues Causeterie, de la Calligarie, au change ou rue des Grands Changes</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1525, t. 5 Inventaire de 1695 "Avignon 4e tiroir 13e-15e liasses".</html:p>
