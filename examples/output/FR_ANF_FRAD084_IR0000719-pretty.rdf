      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>"Livre contenant divers arrentements passés par Louis d'Ancézune et Philiberte de Clermont, Louise, Françoise et Anne d'Ancézune, ensuite Rostain Cadart d'Ancézune"</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Inventaire de 1695, t. 9, fol. 26 v°-27 v°.</html:p>
         </html:div>
