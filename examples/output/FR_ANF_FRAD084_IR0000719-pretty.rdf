      <rico:hasOrHadTitle>
         <rico:Title>
            <rico:textualValue>Procès de Marie-Gabrielle Sinety pour usurpation de créments et enlèvement d'arbres par des habitants de Codolet</rico:textualValue>
         </rico:Title>
      </rico:hasOrHadTitle>
      <rico:history rdf:parseType="XMLLiteral">
         <html:div xmlns:html="http://www.w3.org/1999/xhtml/">
            <html:head>Historique de la conservation</html:head>
            <html:p>Nouvel inventaire de 1807, p. 239 "15e liasse de Codolet et ses îles".</html:p>
         </html:div>
